{
	"compilerOptions": {
		"target": "ESNext",
		"jsx": "react",
		"module": "commonjs",
		"baseUrl": ".",
		"typeRoots": ["./node_internals/@types", "./src/@types"],
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true
